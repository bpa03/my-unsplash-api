{
  "name": "my-unsplash",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:backend": "NODE_ENV=development dotenv -e ./.env -- ts-node-dev --ignore-watch node_modules ./src/app/start.ts",
    "test": "npm run test:unit && npm run test:features",
    "test:unit": "NODE_ENV=test jest",
    "test:features": "NODE_ENV=test dotenv -e ./.env.test -- cucumber-js",
    "pretest": "npm run migrate:test && npm run seed:test",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "migrate:dev": "NODE_ENV=development dotenv -e ./.env -- npx prisma migrate dev",
    "migrate:test": "NODE_ENV=test dotenv -e ./.env.test -- prisma migrate dev",
    "seed:dev": "dotenv -e ./.env -- prisma db seed -- --environment development",
    "seed:test": "dotenv -e ./.env.test -- prisma db seed -- --environment test"
  },
  "prisma": {
    "schema": "./src/Context/Shared/infrastructure/persistence/prisma/schema.prisma",
    "seed": "ts-node ./src/Context/Shared/infrastructure/persistence/prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bson": "^6.2.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-status": "^1.7.3",
    "jose": "^5.1.3",
    "node-dependency-injection": "3.1.2",
    "uuid": "^9.0.1",
    "uuid-validate": "^0.0.3"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.0.1",
    "@faker-js/faker": "^8.3.1",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prisma": "^5.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
